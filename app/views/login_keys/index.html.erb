<h1>Listing login_keys</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Password</th>
    <th>Email</th>
    <th>Security question</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @login_keys.each do |login_key| %>
  <tr>
    <td><%= login_key.Name %></td>
    <td><%= login_key.Password %></td>
    <td><%= login_key.Email %></td>
    <td><%= login_key.Security_Question %></td>
    <td><%= link_to 'Show', login_key %></td>
    <td><%= link_to 'Edit', edit_login_key_path(login_key) %></td>
    <td><%= link_to 'Destroy', login_key, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Login key', new_login_key_path %>
