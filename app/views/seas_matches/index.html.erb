<h1>Listing seas_matches</h1>

<table>
  <tr>
    <th>name</th>
    <th>Graduation year</th>
    <th>Major</th>
    <th>Minor</th>
    <th>Description</th>
    <th>Interests</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seas_matches.each do |seas_match| %>
  <tr>
    <td><%= seas_match.name %></td>
    <td><%= seas_match.Graduation_Year %></td>
    <td><%= seas_match.Major %></td>
    <td><%= seas_match.Minor %></td>
    <td><%= seas_match.Description %></td>
    <td><%= seas_match.Interests %></td>
    <td><%= link_to 'Show', seas_match %></td>
    <td><%= link_to 'Edit', edit_seas_match_path(seas_match) %></td>
    <td><%= link_to 'Destroy', seas_match, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

